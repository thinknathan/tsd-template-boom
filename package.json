{
	"name": "tsd-boom-template",
	"version": "1.0.6",
	"description": "Defold game template with TypeScriptToLua and Boom",
	"repository": "github:thinknathan/tsd-template-boom",
	"keywords": [
		"typescript",
		"lua",
		"defold"
	],
	"author": "Nathan Bolton (https://thinknathan.ca)",
	"license": "MIT",
	"engines": {
		"node": "^16.0.0 || >=18.0.0"
	},
	"type": "module",
	"scripts": {
		"build": "tstl",
		"buildO1": "tstl -p tsconfig.production.json",
		"dev": "tstl --watch",
		"lint": "eslint src",
		"lintfix": "eslint src --fix",
		"prettier": "prettier \"./**/*.{eslintrc,yml,json,ts,md,d.ts}\" --write",
		"resolve": "library resolve",
		"xtgen": "xtgen"
	},
	"devDependencies": {
		"@ts-defold/library": "~0.1.2",
		"@ts-defold/tstl-export-as-global": "~1.0.0",
		"@ts-defold/tstl-userdata-sugar": "~1.0.0",
		"@ts-defold/types": "git+https://git@github.com/thinknathan/ts-defold-types.git#^1.3.2",
		"@typescript-eslint/eslint-plugin": "~6.16.0",
		"@typescript-eslint/parser": "~6.16.0",
		"eslint": "~8.56.0",
		"eslint-config-prettier": "~9.1.0",
		"eslint-plugin-prettier": "~5.1.0",
		"prettier": "~3.1.0",
		"tsd-ext-type-gen": "git+https://git@github.com/thinknathan/tsd-ext-type-gen.git#^1.0.0",
		"tstl-const-propagation": "git+https://git@github.com/thinknathan/tstl-const-propagation.git#^2.0.0",
		"tstl-trim-extensions": "~1.0.3",
		"typescript": "~5.3.3",
		"typescript-to-lua": "~1.23.0",
		"typescript-tstl-plugin": "~0.3.2"
	},
	"resolutions": {
		"typescript-to-lua": "~1.23.0"
	}
}
